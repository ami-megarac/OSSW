				reg = <0x1e782000 0x90>;
				interrupts = <0x10 0x11 0x12 0x23 0x24 0x25 0x26 0x27>;
				clocks = <0x2 0x1a>;
				clock-names = "PCLK";
			};

			serial@1e783000 {
				compatible = "ns16550a";
				reg = <0x1e783000 0x20>;
				reg-shift = <0x2>;
				interrupts = <0x9>;
				clocks = <0x2 0xd>;
				resets = <0xe 0x4>;
				no-loopback-test;
				status = "disabled";
			};

			serial@1e784000 {
