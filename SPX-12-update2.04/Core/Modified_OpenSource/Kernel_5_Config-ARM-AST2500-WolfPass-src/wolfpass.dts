/dts-v1/;

/ {
	model = "AST2500 EVB";
	compatible = "aspeed,ast2500";
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	interrupt-parent = <0x1>;

	aliases {
		serial0 = "/ahb/apb/serial@1e783000";
		serial1 = "/ahb/apb/serial@1e78d000";
		serial2 = "/ahb/apb/serial@1e78e000";
		serial3 = "/ahb/apb/serial@1e78f000";
		serial4 = "/ahb/apb/serial@1e784000";
		serial5 = "/ahb/apb/serial@1e787000";
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu@0 {
			compatible = "arm,arm1176jzf-s";
			device_type = "cpu";
			reg = <0x0>;
		};
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x20000000>;
	};

	sdram@1e6e0000 {
		compatible = "aspeed,ast2500-sdram-edac";
		reg = <0x1e6e0000 0x174>;
		interrupts = <0x0>;
		status = "disabled";
	};

	ahb {
		compatible = "simple-bus";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		spi@1e620000 {
			reg = <0x1e620000 0xc4 0x20000000 0x10000000>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "aspeed,ast2500-fmc";
			clocks = <0x2 0x19>;
			status = "okay";
			interrupts = <0x13>;


			flash@0 {
				reg = <0x0>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "okay";
				m25p,fast-read;
				label = "spx";
    				partitions {
        				compatible = "ami,spx-fmh";
    			};
			};

			flash@1 {
				reg = <0x1>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "disabled";
			};

			flash@2 {
				reg = <0x2>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "disabled";
			};
		};

		spi@1e630000 {
			reg = <0x1e630000 0xc4 0x30000000 0x8000000>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "aspeed,ast2500-spi";
			clocks = <0x2 0x19>;
			status = "okay";

			flash@0 {
				reg = <0x0>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x5f5e100>;
				status = "okay";
				m25p,fast-read;
				label = "pnor";
			};

			flash@1 {
				reg = <0x1>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "disabled";
			};
		};

		spi@1e631000 {
			reg = <0x1e631000 0xc4 0x38000000 0x8000000>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "aspeed,ast2500-spi";
			clocks = <0x2 0x19>;
			status = "okay";

			flash@0 {
				reg = <0x0>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "disabled";
			};

			flash@1 {
				reg = <0x1>;
				compatible = "jedec,spi-nor";
				spi-max-frequency = <0x2faf080>;
				status = "disabled";
			};
		};

		interrupt-controller@1e6c0080 {
			compatible = "aspeed,ast2400-vic";
			interrupt-controller;
			#interrupt-cells = <0x1>;
			valid-sources = <0xfefff7ff 0x807ffff>;
			reg = <0x1e6c0080 0x80>;
			phandle = <0x1>;
		};


		ethernet@1e660000 {
			compatible = "aspeed,ast2500-mac", "faraday,ftgmac100";
			reg = <0x1e660000 0x180>;
			interrupts = <0x2>;
			clocks = <0x2 0x11>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <0x3 0x4>;
		};

		ethernet@1e680000 {
			compatible = "aspeed,ast2500-mac", "faraday,ftgmac100";
			reg = <0x1e680000 0x180>;
			interrupts = <0x3>;
			clocks = <0x2 0x12>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <0x5 0x6>;
		};


		apb {
